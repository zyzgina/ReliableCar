<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/button_frame5_gray_bgwhite"
    android:gravity="center"
    android:orientation="vertical">

    <com.kaopujinfu.appsys.customlayoutlibrary.view.RealIconView
        android:id="@+id/loading_realiconview"
        android:layout_width="90dp"
        android:layout_height="90dp" />

    <TextView
        android:id="@+id/textLoading"
        style="@style/textview_color_back_gray_singleLine_size17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_measure_20sp"
        android:text="加载中..." />
</LinearLayout>
