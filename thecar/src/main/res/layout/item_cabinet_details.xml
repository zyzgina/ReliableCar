<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cab_plain"
    android:paddingBottom="1dp"
    android:paddingRight="1dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <TextView
            android:id="@+id/codeCabinetDetails"
            style="@style/textview_color_back_gray_size15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-5dp"
            android:layout_marginTop="-5dp"
            android:background="@drawable/button_circular5_cabinet_gray"
            android:paddingBottom="@dimen/common_measure_10sp"
            android:paddingLeft="@dimen/common_measure_20sp"
            android:paddingRight="@dimen/common_measure_10sp"
            android:paddingTop="@dimen/common_measure_20sp"
            android:text="0001"
            android:textColor="@color/back" />

        <TextView
            android:id="@+id/supCabinetDetails"
            style="@style/textview_color_white_size15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@color/blue_light"
            android:paddingBottom="@dimen/common_measure_10sp"
            android:paddingLeft="@dimen/common_measure_10sp"
            android:paddingRight="@dimen/common_measure_10sp"
            android:paddingTop="@dimen/common_measure_10sp"
            android:text="自动监管" />

        <TextView
            android:id="@+id/brandCabinetDetails"
            style="@style/textview_color_back_gray_singleLine_size17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/codeCabinetDetails"
            android:layout_marginBottom="@dimen/common_measure_20sp"
            android:layout_marginLeft="@dimen/common_measure_30sp"
            android:layout_marginTop="@dimen/common_measure_20sp"/>

        <TextView
            android:id="@+id/vinCabinetDetails"
            style="@style/textview_color_back_gray_singleLine_size17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/codeCabinetDetails"
            android:layout_marginBottom="@dimen/common_measure_20sp"
            android:layout_marginRight="@dimen/common_measure_20sp"
            android:layout_marginTop="@dimen/common_measure_20sp"/>
    </RelativeLayout>
</RelativeLayout>