<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:id="@+id/parameterConfigInfo_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/common_measure_10sp"
        android:layout_marginTop="@dimen/common_measure_20sp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="13年0个月"
        android:textColor="@color/back"
        android:textSize="@dimen/sp20" />

    <TextView
        android:id="@+id/parameterConfigExplain_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/common_measure_20sp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="2004年4月上牌"
        android:textColor="@color/gray"
        android:textSize="@dimen/sp18" />

</LinearLayout>